  },
  "homepage": "https://github.com/comunion-io/front-next#readme",
  "dependencies": {
    "@comunion/components": "workspace:^0.0.1",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "postcss-nested": "^5.0.6",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-windicss": "^1.8.3",
